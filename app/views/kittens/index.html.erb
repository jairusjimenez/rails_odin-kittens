<h1>All Kittens</h1>

<%= link_to "Create a new kitten record", new_kitten_path %>

<ul>
	<% @kittens.each do |kitten| %>
		<li>
			<%= link_to kitten.name, kitten %> [<%= link_to 'Edit', edit_kitten_path(kitten) %>] [<%= link_to 'Delete', kitten, method: :delete, data: { confirm: 'Are you sure?' } %>]
		</li>
	<% end %>
</ul>